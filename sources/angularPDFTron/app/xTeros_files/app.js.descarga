e.put("app/components/modal/doctor/_modal-doctor-tmpl.html",'<div class="modal-header">\r\n    <button type="button" class="close" ng-click="close()">&times;</button>\r\n\r\n    <div class="row">\r\n        <div class="col-sm-12" ng-if="stage==\'search\'">\r\n            <h1>Add\r\n                <mark class="lime">Doctor</mark>\r\n            </h1>\r\n        </div>\r\n        <div class="col-sm-12" ng-if="stage==\'detail\'">\r\n            <h1><mark class="lime">Doctor</mark> Details</h1>\r\n        </div>\r\n    </div>\r\n</div>\r\n\r\n<div class="modal-body" ng-if="stage==\'search\'">\r\n\r\n    <div style="padding: 25px 15px 0; margin-bottom: -10px">\r\n        <form class="form-horizontal">\r\n            <div class="form-group">\r\n                <div class="row">\r\n                    <div class="col-sm-6">\r\n                        <input\r\n                                type="text"\r\n                                class="form-control search-input"\r\n                                placeholder="1. Add a Doctor"\r\n                                ng-model="searchDoctor.keyword"\r\n                                ng-change = "searchDoctor.search(searchDoctor.keyword,searchDoctor.state)"\r\n                        >\r\n                        <div class="arrow-down-doctor"></div>\r\n                    </div>\r\n                    <div class="col-sm-6 your-drugs">\r\n                        <div class="form-group">\r\n                            <div class="col-sm-12">\r\n                                <select style="visibility: hidden!important" class="form-control search-input" id="state"\r\n                                        ng-change="searchDoctor.search(searchDoctor.keyword,searchDoctor.state)"\r\n                                        name="state" ng-model="searchDoctor.state">\r\n                                    <option value="AK">Alaska</option>\r\n                                    <option value="AL">Alabama</option>\r\n          