12">\r\n                            <table class="table table-material">\r\n                                <thead>\r\n                                <tr>\r\n                                    <th>Drug</th>\r\n                                    <th>Form</th>\r\n                                    <th>Dose</th>\r\n                                    <th>Fill/Qt</th>\r\n                                    <th>Frequency</th>\r\n                                    <th>Member</th>\r\n                                </tr>\r\n                                </thead>\r\n                                <tbody>\r\n                                <tr ng-repeat="drug in drugs">\r\n                                    <td ng-bind="drug.name" style="text-transform: capitalize!important"></td>\r\n                                    <td>\r\n                                        <select class="form-control" ng-model="drug.form" ng-change="drug.update()"\r\n                                                ng-options="form for form in drug.forms">\r\n                                        </select>\r\n                                    </td>\r\n                                    <td>\r\n                                        <select class="form-control" ng-model="drug.dose" ng-change="drug.updateQty()"\r\n                                                ng-options="dose for dose in drug.doses">\r\n                                        </select>\r\n                                    </td>\r\n                                    <td>\r\n                                        <select class="form-control" ng-model="drug.quantity"\r\n                                                ng-options="quantity for quantity in drug.quantities" ng-change="drug.updatePrice()">\r\n                                        </select>\r\n                                    </td>\r\n                                    <td>\r\n                                        <select class="form-control" ng-mo