e.put("app/components/account/profile/guide/account-profile-guide-tmpl.html",'\r\n<div style="min-height: 700px;">\r\n\r\n</div>\r\n<div class="register-account" style="position: absolute;left:0;top:0;right:0;bottom: 0;z-index:1024;">\r\n\r\n    <!-- top menu, header, etc -->\r\n    <!--//= templates/_header.html-->\r\n    <!-- top menu, header, etc -->\r\n    <!--<header ng-include="\'/view/partials/_header-logout.html\'"></header>-->\r\n\r\n    <!-- content -->\r\n    <h1>Let\'s Get Started!</h1>\r\n\r\n    <div class="register-account-container" style="">\r\n        <div class="register-account-content">\r\n\r\n            <div class="row">\r\n                <div class="col-sm-12">\r\n                    <div class="setup-wizard">\r\n                        <form name="setupWizardForm">\r\n                            <p class="main-wizard-text">My zip code is <input type="text"\r\n                                                                              placeholder="20854"\r\n                                                                              class="zipcode-input"\r\n                                                                              ng-class="{\r\n                                        \'error\': setupWizardForm.zipcode.$touched && zip == \'\',\r\n                                        \'not-find\' :  setupWizardForm.zipcode.$touched && zip != \'\' && zipErr\r\n                                       }"\r\n                                                                              ng-pattern="/[0-9]/"\r\n                                                                              ng-model="zip"\r\n                                                                              ng-change="changeZipCode(zip)"\r\n                                                                              name="zipcode"\r\n                                                                              minlength="5"\r\n                                       