                    </div>\r\n\r\n                        <div class="col col-md-2" ng-hide="display.showAllPlans" style="margin-top:10px">\r\n                            <button class="btn btn-primary" ng-click="goBackToAllPlan()" >&larr; Back to All Plans</button>\r\n                        </div>\r\n\r\n                        <div class="col col-md-8 text-center">\r\n                            <h3 uib-tooltip uib-tooltip-html="landscapeDescription" tooltip-placement="bottom">{{selectedRule.label}}\r\n                                </h3>\r\n                        </div>\r\n                    </div>\r\n                    <div class="clearfix small"></div>\r\n                    <div class="row">\r\n\r\n                        <div class="col-md-4 col-md-offset-0 all-plans-sort-by" ng-show="display.showAllPlans"  style="margin-left: 60px;">\r\n                            <div class="form-group">\r\n                                <select name="planSelectFilter" id="planSelectFilter" class="form-control"\r\n                                        ng-model="selectedRule" ng-change="sortBy(selectedRule)" ng-options="opt as opt.label for opt in sortRules">\r\n                                </select>\r\n                            </div>\r\n                        </div>\r\n\r\n\r\n\r\n                    </div>\r\n\r\n\r\n                    <div  ng-show="display.showAllPlans">\r\n\r\n                        <div class="row">\r\n                            <div class="col-sm-12 text-right pagination-no-top">\r\n                                <uib-pagination total-items="bigTotalItems" ng-model="bigCurrentPage" max-size="maxSize" class="pagination-sm" boundary-links="true" force-ellipses="true"></uib-pagination>\r\n                            </div>\r\n                        </div>\r\n\r\n\r\n                        <div class="row select-your-plan" ng-repeat="plan in alls"\r\n                             ng-if="$index>=(bigCurrentPage-1)*10&&$index<bigCurrentPage*10"\r\n                        >\r\n                            <div class="col-sm-12">\r\n                                <div ng-include="\'app/components/plan/_all-plan-item.html\'" ng-controller="PlanSelectController"></div>\r\n\r\n                                <div class="loading-detail" ng-show="plan.isLoading"></div>\r\n                                <div class="plans-tabs plans-tabs-single" ng-if="plan.isLoaded && plan.show" ng-include="\'app/components/plan/_plan-detail.html\'"></div>\r\n                            </div>\r\n                        </div>\r\n\r\n                        <div class="row">\r\n                            <div class="col-sm-12 text-right">\r\n                                <uib-pagination total-items="bigTotalItems" ng-model="bigCurrentPage" max-size="maxSize" class="pagination-sm" boundary-links="true" force-ellipses="true"></uib-pagination>\r\n                            </div>\r\n                        </div>\r\n\r\n                    </div>\r\n\r\n                    <div ng-show="!display.showAllPlans" ng-include="\'app/components/plan/_plan-compare.html\'"></div>\r\n\r\n                </div>\r\n\r\n            </div>\r\n        </div>\r\n    </div>\r\n\r\n    <div class="right-side-bar" ng-include="\'app/components/company/partials/_right-sidebar-business-plan.html\'" ng-controller="CompanyRightMenuController">\r\n    </div>\r\n</div>\r\n\r\n'),e.put("app/components/employee/plan/tree/employee-plan-tree-tmpl.html",'<!--<div class="col-sm-3 col-md-2 left-sidebar"-->\r\n<!--ng-include="\'/view/partials/_left-sidebar.html\'"-->\r\n<!--ng-controller="MenuController"></div>-->\r\n\r\n\r\n<div class="col-md-12 col-sm-12 content has-right-side-bar">\r\n\r\n    <div ng-include="\'app/xteros/components/content-tabs/_content-tabs.html\'"></div>\r\n\r\n    <div class="row">\r\n        <div class="col-md-12">\r\n            <div class="page-content" id="dashboard-content"\r\n                 ng-class="{\'content-fetching\':loadingMessage }">\r\n\r\n\r\n\r\n\r\n                <div ng-if="loadingMessage" ng-include="\'app/xteros/components/plan-loading/_plan-loading.html\'"\r\n                     class="fetching-loading">\r\n                </div>\r\n\r\n                <div class="row">\r\n                    <div class="col-md-12">&nbsp;</div>\r\n                </div>\r\n\r\n                <div class="row plan-dashboard-main" id="dashboard-tree"\r\n                     ng-controller="PlanTreeController as treeVm"\r\n                     ng-include="\'app/components/plan/_plan-tree.html\'">\r\n                </div>\r\n\r\n\r\n\r\n            </div>\r\n        </div>\r\n    </div>\r\n\r\n\r\n\r\n    <div class="right-side-bar" ng-include="\'app/components/company/partials/_right-sidebar-business-plan.html\'"\r\n         ng-init="isTreeMode = true"\r\n         ng-controller="CompanyRightMenuController">\r\n    </div>\r\n\r\n</div>\r\n\r\n\r\n\r\n'),e.put("app/components/employee/profile/coverage/employee-profile-coverage-tmpl.html",'<div class="col-md-12 col-sm-12 content has-right-side-bar employee-profile-coverage">\r\n    <!--<div ng-include="\'app/xteros/components/content-tabs/_content-stack-tabs.html\'"></div>-->\r\n\r\n    <div class="row">\r\n        <div class="col-md-12">\r\n            <div class="page-content">\r\n                <div class="padding-25px  section-form">\r\n\r\n                    <div class="row">\r\n                        <!-- <div class="col-md-offset-1 col-md-10"> -->\r\n                        <div class="col-md-6 form-horizontal">\r\n                            <h3>Upload Insurance Card Photo</h3>\r\n                            <div class="panel-body">\r\n                                <div class="row">\r\n                                    <div class="col-md-6"  >\r\n                                        <div class="company-logo-page">\r\n                                            <img ng-hide="logo_image" src="/assets/img/your-logo-here.png" alt="Upload your card here"/>\r\n                                            <img ng-show="logo_image" ng-src="{{logo_image.dataURL}}" />\r\n                                        </div>\r\n                                        <!--<p class="text-center">Preview</p>-->\r\n                                    </div>\r\n                                    <div class="col-md-6 company_logo_labels">\r\n                                        <p>Supported file types: png, jpg, bmp</p>\r\n                                    </div>\r\n                                </div>\r\n\r\n                                <div class="row" style="margin:20px 10px 10px 10px;">\r\n                                    <form name="image_form" >\r\n                                        <div class="col-md-5">\r\n\r\n                                            <p >\r\n                                                <input type="file"\r\n                                                       id="file"\r\n                                                       name="file"\r\n                                                       accept="image/*"\r\n                                                       image="logo_image"\r\n                                                       resize-max-height="80"\r\n                                                       resize-quality="0.7"\r\n                                                       style="display: none"\r\n                                                       ng-model="selectedFile"\r\n                                                       ng-change="imageChanged(selectedFile)"\r\n                                                />\r\n                                            </p>\r\n\r\n\r\n                                        </div>\r\n\r\n\r\n                                        <div class="col-md-7">\r\n                                            <!-- Progress Bar -->\r\n                                            <div class="progress" style="opacity:{{uploadProgress}};">\r\n                                                <div class="progress-bar" role="progressbar" aria-valuenow="{{ uploadProgress }}" aria-valuemin="0" aria-valuemax="100" style="width: {{ uploadProgress }}%;text-align:left;padding-left:10px;">\r\n                                                    {{ uploadProgress == 0 ? \'\' : uploadProgress + \'%\' }}\r\n                                                </div>\r\n                                            </div>\r\n                                        </div>\r\n\r\n                                        <div class="col-md-12">\r\n                                            <div class="form-group">\r\n                                                <label for="file" class="btn btn-white" style="padding: 0 10px;">Browse</label>\r\n                                                <button type="submit" class="btn btn-primary" ng-click="upload(logo_image)">Upload Card</button>\r\n\r\n                                                <button type="button" class="btn btn-primary" ng-click="sendTextLink()">Text Link</button>\r\n                                            </div>\r\n                                        </div>\r\n                                    </form>\r\n                                </div>\r\n\r\n\r\n\r\n\r\n                            </div>\r\n\r\n                        </div>\r\n\r\n                        <div class="col-md-6">\r\n\r\n                            <div class="col-md-12">\r\n                                <div class="insurance-card">\r\n                                    <img ng-src="{{imageUrl|toInsuranceCardImage}}" ng-if="imageUrl">\r\n                                </div>\r\n                            </div>\r\n                        </div>\r\n\r\n                        <div class="col-md-12">\r\n                            <h2 class="card-detail-h2">Insurance Card Detail</h2>\r\n                        </div>\r\n\r\n   